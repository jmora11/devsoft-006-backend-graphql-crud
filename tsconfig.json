{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",                                /* Specify what module code is generated. */
    "rootDir": "./src",
    "outDir": "./dist","esModuleInterop": true,            /* Disable resolving symlinks to their realpath. This correlates to the same flag in node. */
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictPropertyInitialization": false
  }
}
